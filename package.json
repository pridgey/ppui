{
  "name": "ppui",
  "version": "0.0.1",
  "description": "A UI Library",
  "main": "./src/app.tsx",
  "scripts": {
    "build": "webpack --config ./webpack.prod.js",
    "build:dev": "webpack --config ./webpack.dev.js",
    "build:watch": "webpack --config ./webpack.dev.js --watch",
    "start": "webpack-dev-server --config ./webpack.dev.js --hot --inline",
    "watch": "webpack --config ./webpack.dev.js --watch",
    "test": "jest",
    "prepare": "npm run build && npm run build:dev",
    "prepublishOnly": "jest",
    "test:watch": "jest --watch",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "typescript-babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx)$"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@pisano/enzyme": "^3.3.0-pisano.public.1",
    "@pisano/enzyme-adapter-react-16": "^1.1.1-pisano.public.1",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "awesome-typescript-loader": "^4.0.1",
    "axe-core": "^3.0.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es3": "^1.0.1",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "img-loader": "^3.0.0",
    "jest": "^22.4.3",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "style-loader": "^0.20.3",
    "styled-components": "^3.2.2",
    "ts-loader": "^4.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "typescript-babel-jest": "^1.0.5",
    "typings-for-css-modules-loader": "^1.7.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "body-parser": "^1.18.3",
    "cookies-js": "^1.2.3"
  },
  "dependencies": {
    "@types/codemirror": "0.0.74",
    "@types/react-codemirror": "^1.0.2",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/showdown": "^1.9.2",
    "autoprefixer": "^9.0.0",
    "codemirror": "^5.46.0",
    "concurrently": "^4.0.1",
    "es6-promise": "^4.2.4",
    "fetch": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "react-codemirror": "^1.0.0",
    "react-dom-factories": "^1.0.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "showdown": "^1.9.0"
  }
}
